# Supabase Configuration
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key
DATABASE_URL="postgresql://postgres:your-password@your-supabase-host:5432/postgres"

# Frontend Environment Variables (Vite requires VITE_ prefix)
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production

# Server Configuration
PORT=3001
NODE_ENV=development

# Docker Configuration
MC_BASE_PORT=25565
RCON_BASE_PORT=25575
QUERY_BASE_PORT=25585

# Minecraft Server Configuration
MC_SERVERS_BASE_PATH=/tmp/mc-servers
MC_PORT_RANGE_START=25565
MC_PORT_RANGE_END=25665

# Admin Bootstrap (change these immediately after first setup)
ADMIN_EMAIL=admin@example.com
ADMIN_BOOTSTRAP_TOKEN=change-this-immediately-after-setup